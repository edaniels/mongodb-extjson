{
  "name": "mongodb-stitch-extjson",
  "version": "1.0.0",
  "description": "MongoDB Extended JSON library for Stitch",
  "keywords": [
    "mongodb",
    "json",
    "extended",
    "extjson",
    "parser",
    "serializer",
    "deserializer",
    "stitch"
  ],
  "license": "Apache-2.0",
  "author": "Eric Daniels <eric.daniels@mongodb.com>",
  "files": [
    "lib",
    "index.js",
    "dist"
  ],
  "main": "index.js",
  "module": "dist/ejson.esm.js",
  "browser": {
    "./index.js": "./dist/ejson.browser.umd.js",
    "./dist/ejson.esm.js": "./dist/ejson.browser.esm.js"
  },
  "directories": {
    "lib": "./lib"
  },
  "repository": "edaniels/mongodb-extjson",
  "scripts": {
    "test": "mocha test/",
    "coverage": "istanbul cover _mocha -- --recursive -t --ui tdd test/",
    "lint": "eslint lib test",
    "format": "prettier --print-width 100 --tab-width 2 --single-quote --write 'test/**/*.js' 'lib/**/*.js'",
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "dependencies": {
    "mongodb-stitch-bson": "1.0.0",
    "buffer": "^5.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "conventional-changelog-cli": "^1.3.5",
    "eslint": "^4.7.2",
    "eslint-plugin-prettier": "^2.3.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.1",
    "mongodb": "^2.2.27",
    "prettier": "~1.12.0",
    "rollup": "^0.56.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-resolve": "^3.0.3"
  }
}
